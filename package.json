{
  "name": "es6-coding-exercises",
  "private": true,
  "description": "Coding exercises written in ES6.",
  "license": "ISC",
  "repository": "amercier/es6-coding-exercises",
  "author": "Alex Mercier <pro.alexandre.mercier@gmail.com> (http://amercier.com)",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "transpile": "rimraf dist && babel src -d dist",
    "prepublish": "npm run transpile",
    "doc": "rimraf esdoc && esdoc -c esdoc.json",
    "lint": "eslint .",
    "mocha": "mocha specs --recursive --compilers js:babel-register",
    "start": "export NODE_ENV=development; npm run mocha -- --watch --growl",
    "test": "export NODE_ENV=test; npm run lint && rimraf coverage && nyc npm run mocha"
  },
  "main": "dist/lib/index.js",
  "keywords": [
    "es6",
    "javascript",
    "coding",
    "exercises",
    "tests",
    "mocha"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.8.0",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.0",
    "codecov": "^1.0.1",
    "esdoc": "^0.4.7",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "mocha": "^3.1.2",
    "nyc": "^8.3.2",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4"
  },
  "nyc": {
    "exclude": "**",
    "reporter": [
      "lcovonly",
      "html",
      "text"
    ]
  }
}
